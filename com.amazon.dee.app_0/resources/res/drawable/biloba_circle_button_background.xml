<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="false">
        <shape android:shape="oval">
            <stroke android:width="1dp" android:color="?attr/mosaicNeutral50"/>
            <solid android:color="?attr/mosaicNeutral50"/>
            <size android:height="60dp" android:width="60dp"/>
        </shape>
    </item>
    <item android:state_selected="true">
        <shape android:shape="oval">
            <stroke android:width="1dp" android:color="?attr/mosaicNeutral50"/>
            <solid android:color="?attr/mosaicNeutral50"/>
            <size android:height="60dp" android:width="60dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <stroke android:width="1dp" android:color="?attr/mosaicNeutral40"/>
            <solid android:color="?attr/mosaicNeutral40"/>
            <size android:height="60dp" android:width="60dp"/>
        </shape>
    </item>
</selector>
